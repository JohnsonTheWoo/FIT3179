{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
        "url": "your_data.json", // Replace with your data source URL
        "format": {"type": "json"}
    },
    "mark": "geoshape",
    "projection": {"type": "identity"},
    "encoding": {
        "color": {
            "field": "Average Calorie Intake",
            "type": "quantitative",
            "scale": {"type": "linear", "domain": [minValue, maxValue], "scheme": "viridis"} // Adjust the domain and color scheme
        },
        "tooltip": [
            {"field": "Country", "type": "nominal"},
            {"field": "Average Calorie Intake", "type": "quantitative"}
        ]
    }
}
